            <string>//Pause For StartDelay
if(startDelay &gt; 0)
{
    startDelay -= 1;
    textBox.printString = " ";
    exit;
}

//Detect Conversation changes
if(obj_player.use &amp;&amp; textBox.printCount == string_length(textBox.printString)) //keyboard_check_pressed(vk_left)
{
    conNumber += 1;
}

//Conversation List
if(conNumber != lastConNumber &amp;&amp; conNumber &lt; array_length_1d(conversation))
{
    textBox.printString = conversation[conNumber];
}
else if(conNumber != lastConNumber &amp;&amp; !finished)
{
    obj_player.goodByeAlarm = obj_player.goodByeTime;
    textBox.printString = " ";
    finished = true;
}   

//Death Operation
if(finished)
{
    finTimer += 1;
    
    //Spawn Transition
    if(finTimer == 50)
    {
        instance_create(x,y,obj_fadeInElevator);
    }
}

//Manipulate Conversation Animation
if(textBox.printCount != lastPrintCount &amp;&amp; textBox.printCount != string_length(textBox.printString) &amp;&amp; textBox.printAlarm &lt;= textBox.printRate)
{
    imageSpeed = animationSpeed; //.2;
}
else if(textBox.printCount == string_length(textBox.printString) || textBox.printAlarm &gt; textBox.printRate)
{
    imageSpeed = 0;
    //image_index = 0;
}
lastPrintCount = textBox.printCount;

//Manage Smooth Animation
if(imageSpeed &gt; 0)
{
    image_speed = imageSpeed;
}
else if(imageSpeed == 0 &amp;&amp; floor(image_index) == 0)
{
    image_speed = 0;
}

//Manage ConvPhase (For Player Icon)
//Draw Nothing (Conversation Over)
if(finished)
{
    convPhase = -1;
}
//Goodbye Input
else if(conNumber == array_length_1d(conversation))
{
    convPhase = 2;
}
//General Input Required
else if(textBox.printCount == string_length(textBox.printString) &amp;&amp; textBox.printString != " ")
{
    convPhase = 0;
}
//Draw Ellipses
else
{
    convPhase = 1;
}


//Scan LastConNumber Variable
lastConNumber = conNumber;
</string>
