          <argument>
            <kind>1</kind>
            <string>

//Trigger Spikes
if(position_meeting(x+10,y+10,obj_player) &amp;&amp; !obj_player.dead &amp;&amp; !triggered)
{
    spikeAlarm = spikeDelay+spikeCooldown;
    triggered = true;
}

//Manage Spikes
if(triggered &amp;&amp; spikeAlarm &gt; 0)
{
    spikeAlarm -= 1;
    
    //Manage Spikes
    if(spikeAlarm &gt; spikeCooldown)
    {
        imageIndex = 2;
        sprite_index = -1; //-1
    }
    else
    {
        imageIndex = 2;
        sprite_index = spr_spikes;
    }
}
else
{   
    imageIndex = 0;
    sprite_index = -1;
    triggered = false;
}


</string>
