triggered = false;
spikeAlarm = 0;
spikeDelay = 20;
spikeCooldown = 20;
spikeHeightOffSet = 4; //Starting Position Of Rising Spike

//Create Delay Array (For Smoother Animation)
delayCount = 0;
for(i = 0;i &lt; spikeDelay;i++)
