            <string>//Gravity
if(!place_meeting(x,y,obj_wall))
{
    if(gravAlarm &gt; 0)
    {
        gravAlarm -= 1;
    }
    else
    {   
        gravAlarm = gravRate;
        vSpeed += grav;
    }
}   

//Vertical Speed Manager
if(vSpeed != 0)
{
    if(!place_meeting(x,y+vSpeed,obj_wall))
    {
        y += vSpeed;
    }   
    else if(!place_meeting(x,y+sign(vSpeed),obj_wall))
    {
        do
        {
            y += sign(vSpeed);
        }
        until place_meeting(x,y+sign(vSpeed),obj_wall);
    }
    
    if(place_meeting(x,y+1,obj_wall))
    {
        vSpeed = -jumpSpeed;
    }
    else if(place_meeting(x,y-1,obj_wall))
    {
        vSpeed = 0;
    }
}
