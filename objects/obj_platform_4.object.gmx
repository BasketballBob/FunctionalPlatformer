            <kind>1</kind>
            <string>//THE FOLLOWING PLATFORMING CODE EXISTS THANKS TO
//SHAUN SPAULDING AND HIS PLATFORM TUTORIAL
sprite_index = -1;
image_speed = 0;
depth = 2;

//Tile Set Variables
width = floor(image_xscale);
xp = 0;
tileSize = 20;
tileSprite = 0;


for(xp = 0;xp &lt; width;xp++)
{
    //Left
    if(xp == 0 &amp;&amp; !position_meeting(x-1,y,obj_wall))
    {
        tileSprite = 0;
        
        //Insert Edge Visual Tile
        tile_add(bg_platformTile_4,3*20,0,tileSize,tileSize,x-tileSize,y,1);
    }
    //Right
    else if(xp == width-1 &amp;&amp; !position_meeting(x+tileSize*width+1,y,obj_wall))
    {
        tileSprite = 2;
        
        //Insert Edge Visual Tile
        tile_add(bg_platformTile_4,4*20,0,tileSize,tileSize,x+tileSize*width,y,1);
    }
    //Middle
    else
    {
        tileSprite = 1;
    }
    
    tile_add(bg_platformTile_4,tileSprite*tileSize,0,tileSize,tileSize,x+xp*tileSize,y,1);
}

//Allow Platform To Collide
sprite_index = object_get_sprite(obj_platform);
</string>
          </argument>
